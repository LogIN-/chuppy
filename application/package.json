{
  "name": "Chuppy",
  "main": "./index.html",
  "description": "Chuppy is an fresh open-source approach for data management. It gives you maximum freedom and control over your own projects and data.",
  "version": "0.1.0",
  "private": true,
  "keywords": [
    "Chuppy"
  ],
  "no-edit-menu": true,
  "single-instance": false,
  "window": {
    "icon": "/lib/images/icon.png",
    "toolbar": false,
    "frame": false,
    "position": "center",
    "width": 1024,
    "height": 768,
    "min_width": 1024,
    "min_height": 768,
    "resizable": true
  },
  "webkit": {
    "plugin": true
  },
  "user-agent": "Mozilla/5.0 (compatible; Chuppy/0.1.0; +https://github.com/LogIN-/chuppy)",
  "dependencies": {
    "adm-zip": "^0.4.4",
    "async": "^0.9.0",
    "bcrypt-nodejs": "0.0.3",
    "bookshelf": "^0.7.7",
    "cookies": "^0.5.0",
    "deck": "latest",
    "freeport": "^1.0.3",
    "ftp-server": "^0.1.0",
    "gaze": "^0.6.4",
    "geoip-lite": "^1.1.3",
    "i18n": "^0.4.1",
    "keygrip": "^1.0.1",
    "knex": "^0.6.22",
    "localtunnel": "^1.3.0",
    "mime": "^1.2.11",
    "moment": "^2.8.1",
    "nedb": "0.10.7",
    "open": "^0.0.5",
    "routes": "^1.2.0",
    "sqlite3": "^2.2.4",
    "watch": "^0.10.0"
  },
  "devDependencies": {
    "grunt-contrib-jshint": "~0.6.4",
    "grunt-contrib-nodeunit": "~0.2.0",
    "grunt-contrib-watch": "~0.5.3",
    "grunt": "^0.4.5",
    "grunt-node-webkit-builder": "^0.1.21",
    "grunt-contrib-csslint": "^0.2.0"
  },
  "bugs": "https://github.com/LogIN-/chuppy/issues",
  "maintainers": [
    {
      "name": "LogIN",
      "email": "info@ivantomic.com",
      "web": "http://ivantomic.com"
    }
  ],
  "licenses": [
    {
      "type": "The MIT License (MIT)",
      "url": "https://github.com/LogIN-/chuppy/blob/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/LogIN-/chuppy.git"
  },
  "chromium-args": "--enable-threaded-compositing --enable-usermedia-screen-capturing"
}
